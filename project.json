{
    "packages": {
        "asm": {
            "pkg": "bluet/berry/asm/",
            "output": "asm.jar"
        },
        "analyzer": {
            "dep": [
                "asm"
            ],
            "pkg": "bluet/berry/berrytool/",
            "output": "analyzer.jar"
        }
    },
    "runs": {
        "download_manifest": {
            "function": "download_manifest",
            "repeat_everytime": false
        },
        "download_minecraft": {
            "dep": [
                "download_manifest"
            ],
            "function": "download_minecraft",
            "repeat_everytime": false
        },
        "deobfuscate": {
            "dep": [
                "download_minecraft"
            ],
            "function": "deobfuscate",
            "repeat_everytime": false
        },
        "download_assets": {
            "dep": [
                "download_minecraft"
            ],
            "function": "download_assets",
            "repeat_everytime": false
        },
        "build_all": {
            "dep": [
                "asm",
                "analyzer"
            ]
        }
    }
}
